	<params>
		<param name="toolbar" type="list" default="Compact" label="Function" description="function">
			<option value="Advanced">Advanced</option>
			<option value="Compact">Compact</option>
			<option value="Basic">Basic</option>
		</param>
		<param name="skin" type="list" default="default" label="Skin" description="The skin of Editor">
			<option value="default">default</option>
			<option value="office2003">office2003</option>
			<option value="silver">silver</option>
		</param>
		<param name="content_css" type="radio" default="0" label="Use Template CSS classes?" description="Load CSS classes from template_css.css">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
 		<param name="content_css_custom" type="text" default="" label="Use Custom CSS Classes?" description="Specify a custom CSS file to load - simply enter the FULL path to the css file you want loaded"/>
 		<param name="text_direction" type="list" default="ltr" label="Text Direction" description="Change the text display direction">
 			<option value="ltr">Left to Right</option>
 			<option value="rtl">Right to Left</option>
 		</param>
		<param name="newlines" type="list" default="false" label="Newlines" description="New lines will be turned into the selected option">
 			<option value="false">P Elements</option>
			<option value="true">BR Elements</option>
 		</param>
		<param name="wwidth" type="text" default="480" label="width" description="The width of Editor"/>
		<param name="hheight" type="text" default="480" label="height" description="The height of Editor"/>
	</params>
